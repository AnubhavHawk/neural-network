  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# KNN ALGORITHM"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# we first calculate the distance of the point (query point) from all other points in the space == O(N) for every point\n",
    "#complexity can be O(Q.N)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 101,
   "metadata": {},
   "outputs": [],
   "source": [
    "#ecludian distance between two points (x1,y1), (x2,y2)\n",
    "def distance(x1, x2):\n",
    "    return np.sqrt(((x1 - x2)**2).sum())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 118,
   "metadata": {},
   "outputs": [],
   "source": [
    "# KNN(training_dataset, query_point, training_labels, value_of_neighbours)\n",
    "#value_of_neighbours shouldn't be too high or too low\n",
    "\n",
    "def KNN(X_train, query_point, Y_train, k =5):\n",
    "    vals = []\n",
    "    \n",
    "    for ix in range(X_train.shape[0]):\n",
    "        #v = [distance_from_the_query_point, label_of_the_point i.e., apple or lemon]\n",
    "        v = [distance(query_point, X_train[ix, :]), Y_train[ix]]\n",
    "        vals.append(v)\n",
    "        updated_vals = sorted(vals)\n",
    "        \n",
    "        pred_arr = np.asarray(updated_vals[:k])\n",
    "        pred_arr = np.unique(pred_arr[:, 1], return_counts=True)\n",
    "        \n",
    "        #get the largest occurence\n",
    "        index = pred_arr[1].argmax() #index of largest frequency\n",
    "        \n",
    "        return pred_arr[0][index]\n",
    "    print(vals)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 120,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "array([0.])"
